{
	"name": "@adonisjs/validator",
	"version": "13.0.2-2",
	"description": "Validator for adonis framework",
	"main": "build/index.js",
	"type": "module",
	"files": [
		"build/src",
		"build/index.d.ts",
		"build/index.js"
	],
	"exports": {
		".": "./build/index.js",
		"./types": "./build/src/types.js"
	},
	"scripts": {
		"pretest": "npm run lint",
		"test": "c8 npm run vscode:test",
		"clean": "del-cli build",
    "typecheck": "tsc --noEmit",
		"compile": "npm run lint && npm run clean && tsc",
		"copy:files": "copyfiles \"templates/**/*.txt\" build",
		"build": "npm run compile",
		"prepublishOnly": "npm run build",
		"release": "np --message=\"chore(release): %s\"",
		"version": "npm run build",
		"benchmarks": "node build/benchmarks/index.js",
		"lint": "eslint . --ext=.ts",
		"format": "prettier --write .",
		"sync-labels": "github-label-sync --labels .github/labels.json adonisjs/validator",
		"vscode:test": "node --loader=ts-node/esm bin/test.ts"
	},
	"devDependencies": {
		"@adonisjs/application": "^7.1.2-3",
		"@adonisjs/bodyparser": "^9.3.2-6",
		"@adonisjs/encryption": "^5.1.2-2",
		"@adonisjs/eslint-config": "^1.1.7",
		"@adonisjs/events": "^8.4.9-3",
		"@adonisjs/fold": "^9.9.3-5",
		"@adonisjs/http-server": "^6.8.2-7",
		"@adonisjs/logger": "^5.4.2-3",
		"@adonisjs/prettier-config": "^1.1.7",
		"@adonisjs/tsconfig": "^1.1.7",
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@japa/assert": "^2.0.0-1",
		"@japa/runner": "^3.0.0-3",
		"@swc/core": "^1.3.67",
		"@types/benchmark": "^2.1.2",
		"@types/node": "^20.3.3",
		"benchmark": "^2.1.4",
		"c8": "^8.0.0",
		"class-validator": "^0.14.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.44.0",
		"execa": "^7.1.1",
		"github-label-sync": "^2.3.1",
		"husky": "^8.0.3",
		"joi": "^17.9.2",
		"kleur": "^4.1.5",
		"np": "^8.0.4",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6",
		"zod": "^3.21.4"
	},
	"dependencies": {
		"@poppinss/utils": "^6.5.0-3",
		"@types/luxon": "^3.3.0",
		"@types/validator": "^13.7.17",
		"luxon": "^3.3.0",
		"normalize-url": "^8.0.0",
		"tmp-cache": "^1.1.0",
		"validator": "^13.9.0"
	},
	"peerDependencies": {
		"@adonisjs/bodyparser": "^9.3.2-5"
	},
	"keywords": [
		"validator",
		"schema-validation",
		"adonisjs",
		"adonisjs-validator"
	],
	"license": "MIT",
	"author": "virk,adonisjs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adonisjs/validator.git"
	},
	"bugs": {
		"url": "https://github.com/adonisjs/validator/issues"
	},
	"homepage": "https://github.com/adonisjs/validator#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"publishConfig": {
		"access": "public",
		"tag": "next"
	},
	"np": {
		"message": "chore(release): %s",
		"tag": "next",
		"branch": "main",
		"anyBranch": false
	},
	"c8": {
		"reporter": [
			"text",
			"html"
		],
		"exclude": [
			"tests/**",
			"test_factories/**",
			".yalc/**"
		]
	},
	"eslintConfig": {
		"extends": "@adonisjs/eslint-config/package"
	},
	"prettier": "@adonisjs/prettier-config"
}
