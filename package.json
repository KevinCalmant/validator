{
  "name": "@adonisjs/validator",
  "version": "7.3.2",
  "description": "Validator for adonis framework",
  "main": "build/providers/ValidatorProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "copy:files": "copyfiles \"templates/**/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "lint": "eslint . --ext=.ts",
    "benchmarks": "node build/benchmarks/index.js"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "author": "virk,adonisjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-validation-provider.git"
  },
  "keywords": [
    "validator",
    "schema-validation",
    "adonis",
    "indicative"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-validation-provider/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-validation-provider#readme",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "devDependencies": {
    "@adonisjs/bodyparser": "^3.2.4",
    "@adonisjs/encryption": "^2.0.4",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/http-server": "^2.1.2",
    "@adonisjs/logger": "^2.0.6",
    "@adonisjs/mrm-preset": "^2.3.0",
    "@adonisjs/profiler": "^3.0.4",
    "@hapi/joi": "^17.1.1",
    "@types/node": "^14.0.13",
    "benchmark": "^2.1.4",
    "class-validator": "^0.12.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.2.0",
    "eslint-plugin-adonis": "^1.0.10",
    "execa": "^4.0.2",
    "husky": "^4.2.5",
    "indicative": "^7.4.4",
    "japa": "^3.1.1",
    "kleur": "^4.0.0",
    "mrm": "^2.3.3",
    "np": "^6.2.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@poppinss/utils": "^2.2.7",
    "@types/luxon": "^1.24.0",
    "@types/validator": "^13.0.0",
    "endent": "^2.0.1",
    "luxon": "^1.24.1",
    "pope": "^2.0.2",
    "validator": "^13.1.1"
  },
  "peerDependencies": {
    "@adonisjs/http-server": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@adonisjs/http-server": {
      "optional": true
    }
  }
}
